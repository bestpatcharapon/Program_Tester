{
  "name": "autotest-center",
  "version": "1.0.0",
  "description": "Enterprise AutoTest Center Dashboard with Evidence Gallery",
  "main": "electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:5173 && electron .\"",
    "backend": "cd backend && uvicorn main:app --reload --port 8000",
    "start:all": "concurrently \"npm run backend\" \"npm run electron:dev\""
  },
  "dependencies": {
    "axios": "^1.6.2",
    "date-fns": "^3.0.0",
    "framer-motion": "^10.16.16",
    "lucide-react": "^0.294.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.0",
    "recharts": "^2.10.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "vite": "^5.0.8",
    "wait-on": "^7.2.0"
  }
}
